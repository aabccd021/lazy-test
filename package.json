{
  "scripts": {
    "build": "tsup --format cjs,esm",
    "do": "sh -c 'ts-node --compilerOptions \"{\\\"isolatedModules\\\": false}\" --eval \"import(\\\"$0\\\").then(a => a.main());\"'",
    "test": "c8 pnpm do ./test/index",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --max-warnings=0 . --fix"
  },
  "sideEffects": false,
  "name": "bxb",
  "repository": "https://github.com/aabccd021/bxb",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "require": "./dist/node.js",
      "import": "./dist/node.mjs"
    }
  },
  "tsup": {
    "entry": {
      "index": "src/index.ts",
      "node": "src/node.ts"
    },
    "sourcemap": true,
    "clean": true
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/node": "^18.11.18",
    "c8": "^7.12.0",
    "eslint": "^8.30.0",
    "fp-ts-contrib": "^0.1.29",
    "nazna": "^1.14.2",
    "semantic-release": "^20.0.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.1",
    "retry-ts": "^0.1.4",
    "spectacles-ts": "^1.0.7",
    "ts-pattern": "^4.0.6"
  }
}
